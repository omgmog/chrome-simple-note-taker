{
    "manifest_version": 2,

    "name": "Take a note",
    "description": "This extension will let you take a simple note in Chrome.",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'"
}
